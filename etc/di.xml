<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <virtualType name="Aheadworks\MobileAppConnector\ReferenceImageUpload" type="Aheadworks\MobileAppConnector\Model\ImageUploader">
        <arguments>
            <argument name="baseTmpPath" xsi:type="string">applogo/tmp</argument>
            <argument name="basePath" xsi:type="string">applogo</argument>
            <argument name="allowedExtensions" xsi:type="array">
                <item name="jpg" xsi:type="string">jpg</item>
                <item name="jpeg" xsi:type="string">jpeg</item>
                <item name="gif" xsi:type="string">gif</item>
                <item name="png" xsi:type="string">png</item>
            </argument>
            <argument name="allowedMimeTypes" xsi:type="array">
                <item name="jpg" xsi:type="string">image/jpg</item>
                <item name="jpeg" xsi:type="string">image/jpeg</item>
                <item name="gif" xsi:type="string">image/gif</item>
                <item name="png" xsi:type="string">image/png</item>
            </argument> 
        </arguments>
    </virtualType>
    <type name="Aheadworks\MobileAppConnector\Controller\Adminhtml\Preferences\Upload">
        <arguments>
            <argument name="imageUploader" xsi:type="object">Aheadworks\MobileAppConnector\ReferenceImageUpload</argument>
        </arguments>
    </type>
    <preference for="Aheadworks\MobileAppConnector\Api\AppPrefrenceDataManagementInterface" type="Aheadworks\MobileAppConnector\Model\AppPrefrenceDataManagement"/>
</config>