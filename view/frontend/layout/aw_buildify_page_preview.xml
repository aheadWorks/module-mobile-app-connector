<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Aheadworks_MobileAppConnector::css/extend-preview.css" />
    </head>
    <body>
        <referenceBlock name="aw.buildify.page.preview" remove="true"/>
        <referenceContainer name="content">
            <block class="Aheadworks\MobileAppConnector\Block\ThirdParty\Buildify\Page\Preview"
                   name="aw.buildify.mobileappconnector.page.preview"
                   template="Aheadworks_MobileAppConnector::third_party/page/preview.phtml" />
        </referenceContainer>
    </body>
</page>
